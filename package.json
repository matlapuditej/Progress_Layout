{
  "name": "progress_layout",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jeet": "^7.2.0",
    "jest-enzyme": "^7.1.2",
    "node-sass": "^7.0.0",
    "node-sass-chokidar": "^1.4.0",
    "react": "^16.12.0",
    "react-circular-progressbar": "^1.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^5.1.2",
    "react-scripts": "2.1.1",
    "redux": "^4.0.4",
    "redux-saga": "^0.16.2",
    "sass-loader": "^7.3.1",
    "semantic-ui-react": "^0.83.0"
  },
  "scripts": {
    "start": "react-scripts start && npm run watch-css",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "run-sass": "node-sass src --watch --output src",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1"
  }
}
